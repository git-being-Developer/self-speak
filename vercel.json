{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(journal|dashboard|billing|auth)/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/",
      "dest": "/landing.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
