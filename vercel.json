{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(journal|dashboard|billing|auth)/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/",
      "dest": "/frontend/landing.html"
    },
    {
      "src": "/(.*\\.(html|css|js|json|png|jpg|svg|ico))",
      "dest": "/frontend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  }
}
